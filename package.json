  },
  "files": [
    "LICENSE",
    "README.md",
    "src/index.js"
  ],
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^11.0.2",
    "conventional-changelog-conventionalcommits": "^9.1.0",
