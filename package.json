{
  "name": "@dendavidov/eslint-config-react",
  "version": "2.0.0",
  "description": "Shareable ESLint config for React + TypeScript (ESLint 9+)",
  "type": "module",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "license": "MIT",
  "author": "Denis Davydov <mail@dendavidov.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:dendavidov/eslint-config-react.git"
  },
  "bugs": {
    "url": "https://github.com/dendavidov/eslint-config-react/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "LICENSE",
    "README.md",
    "src/index.js",
    "src/index.d.ts"
  ],
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --watchAll=false",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "release": "npx semantic-release",
    "prepare": "npx husky install"
  },
  "dependencies": {
    "@eslint/js": "^9.31.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-testing-library": "^7.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@semantic-release/git": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-testing-library": "^7.5.4",
    "husky": "^9.1.7",
    "jest": "^30.0.4",
    "prettier": "^3.6.2"
  }
}
